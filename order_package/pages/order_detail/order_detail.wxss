/* order_package/pages/order_detail/order_detail.wxss */

page {
  background-color: #f3f4f5;
}

/* 顶部地址信息 */

.address {
  display: flex;
  align-items: center;
  background-color: #fff;
  height: 172rpx;
  margin-bottom: 16rpx;
  padding: 0rpx 30rpx;
}

.address-icon image {
  width: 26rpx;
  height: 32rpx;
}

.address-info {
  margin-left: 16rpx;
}

.user-info {
  display: flex;
  align-items: center;
}

.user-name {
  display: inline-block;
  max-width: 380rpx;
  /* background-color: cyan; */
  font-size: 30rpx;
  font-weight: 600;
}

.phone {
  font-size: 26rpx;
  color: #9b9b9b;
  margin-left: 20rpx;
}

.detail-address {
  font-size: 26rpx;
  color: #333;
  margin-top: 6rpx;
}

/* 订单详情 */

.order-detail {
  background-color: #fff;
  padding: 0rpx 30rpx;
}

/* 订单编号 B */

.num-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0rpx;
  border-bottom: 2rpx solid #e9e9e9;
}

.order-num {
  font-size: 26rpx;
  display: flex;
  align-items: center;
}

.order-icon image {
  width: 26rpx;
  height: 30rpx;
}

.order-num1 {
  margin-left: 14rpx;
  color: #333;
  width: 526rpx;
  /* background-color: chartreuse; */
}

.order-status {
  font-size: 26rpx;
  color: #fff;
  background-color: #ff5062;
  width: 106rpx;
  height: 42rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 42rpx;
}

.else-status {
  background-color: #ff8a4f !important;
}

/* 订单编号 End */

.program {
  font-size: 30rpx;
  font-weight: 600;
  padding: 20rpx 0rpx;
}

.base-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 20rpx;
}

.count {
  font-size: 26rpx;
  color: #9b9b9b;
}

.check-form {
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  padding: 0rpx 20rpx;
  font-size: 26rpx;
  color: #2090fe;
  border: 2rpx solid #2090fe;
  border-radius: 25rpx;
}

/* 发报告------物流信息 */

.express {
  margin-top: 16rpx;
  background-color: #fff;
  padding: 16rpx 30rpx;
}

.ex-title {
  font-size: 28rpx;
  font-weight: 600;
  color: #333;
}

.ex-info {
  margin-top: 16rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.ex-com {
  font-size: 26rpx;
  margin-right: 20rpx;
  color: #999;
}

.ex-num {
  color: #2090fe;
  text-decoration: underline;
}

.ex-right {
  font-size: 28rpx;
  color: #fff;
  padding: 6rpx 16rpx;
  border-radius: 8rpx;
  background-color: #2090fe;
  /* border: 2rpx solid #2090FE; */
}

/* 使用优惠券 */

.coupon {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 30rpx;
  height: 143rpx;
  background-color: #fff;
  margin-top: 16rpx;
  font-size: 26rpx;
}

.icon-title {
  display: flex;
  align-items: center;
}

.coupon-title {
  margin-left: 16rpx;
}

.icon image {
  width: 36rpx;
  height: 36rpx;
}

.coupon-money {
  margin-top: 10rpx;
  margin-left: 52rpx;
}

.coupon-money text {
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
}

.coupon-right1 {
  width: 23rpx;
  height: 24rpx;
  border: 4rpx solid #999;
  border-radius: 16rpx;
}

.coupon-right image {
  width: 31rpx;
  height: 32rpx;
}

/* 付款方式 */

.wx-pay {
  padding: 20rpx 30rpx;
  background-color: #fff;
  margin-top: 16rpx;
}

.pay-type {
  font-size: 30rpx;
  font-weight: 600;
}

.pay-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
  padding: 20rpx 0rpx;
  border-bottom: 2rpx solid #e9e9e9;
}

.pay-count {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding-top: 20rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #333;
}

.pay-count text {
  font-size: 34rpx;
  color: #ff5062;
}

.money-count {
  border: none;
}

.money {
  font-size: 28rpx;
}

/* 线下支付 */

.outline-pay {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  background-color: #fff;
  margin-top: 16rpx;
}

.line-left {
  width: 500rpx;
  /* background-color: cadetblue; */
}

.line-title {
  font-size: 30rpx;
  font-weight: 600;
}

.card-num {
  font-size: 26rpx;
  color: #949494;
  padding-top: 10rpx;
}

.up-btn {
  font-size: 26rpx;
  width: 140rpx;
  height: 46rpx;
  border-radius: 25rpx;
  border: 2rpx solid #2090fe;
  color: #2090fe;
  line-height: 46rpx;
  text-align: center;
}

/* 上传凭证展示 */

.proof {
  width: 170rpx;
  height: 170rpx;
  background-color: #f1f1f1;
}

.proof-img {
  width: 170rpx;
  height: 170rpx;
}

.proof {
  position: relative;
}

.del-proof {
  position: absolute;
  right: 0;
  top: 0;
  transform: translate(50%, -50%);
  width: 50rpx;
  height: 50rpx;
  /* background-color: #fff;
  border-radius: 50%; */
}

.del-proof image {
  width: 32rpx;
  height: 32rpx;
}

/* 联系客服的按钮样式 */

.contact {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 10rpx;
}

/* 底部按钮 */

.btn-box {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  background-color: #fff;
  z-index: 100;
}

.btn-box view {
  width: 330rpx;
  height: 72rpx;
  border: 2rpx #2090fe solid;
  border-radius: 36rpx;
  margin-left: 26rpx;
  text-align: center;
  line-height: 72rpx;
  font-size: 28rpx;
}

.btn-center {
  justify-content: center;
}

/* 已完成状态是的按钮 */

.done {
  width: 690rpx !important;
}

.cancel {
  position: relative;
  color: #2090fe;
}

.input-name {
  background-color: #2090fe;
  color: #fff;
  position: relative;
}

/* 删除弹窗 */

.del-mask {
  position: fixed;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  z-index: 10;
}

.del-pop {
  width: 580rpx;
  height: 250rpx;
  background-color: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 14rpx;
}

.del-tips {
  padding: 46rpx 0rpx;
  text-align: center;
  font-size: 32rpx;
  color: #333;
}

.btn-box1 {
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.btn-box1 view {
  width: 180rpx;
  height: 66rpx;
  font-size: 26rpx;
  line-height: 66rpx;
  text-align: center;
  border-radius: 10rpx;
}

.yes {
  background-color: #ff6675;
  color: #fff;
}

.no {
  border: 2rpx solid #666;
  color: #666;
}

/* 下载报告提示弹窗 */

.download {
  position: fixed;
  left: 50%;
  transform: translateX(-50%);
  width: 690rpx;
  height: 600rpx;
  border-radius: 14rpx;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 80rpx rgba(0, 0, 0, 0.2);
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
  transition: all 0.3s ease-in-out;
}

.close-img {
  position: absolute;
  right: 0;
  top: 0;
  width: 36rpx;
  height: 36rpx;
  transform: translate(50%, -50%);
}

.reason {
  width: 600rpx;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  line-height: 46rpx;
  text-align: center;
}

.down-url {
  width: 520rpx;
  height: 280rpx;
  padding: 0rpx 40rpx;
  word-break: break-all;
  word-wrap: break-word;
  font-size: 30rpx;
  color: skyblue;
  line-height: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f1f1f1;
  margin-bottom: 20rpx;
}

.copy-btn {
  width: 600rpx;
  height: 80rpx;
  background-color: #2090fe;
  border-radius: 50rpx;
  text-align: center;
  line-height: 80rpx;
  color: #fff;
  font-size: 28rpx;
}

/* 画布 */

.canvas {
  position: fixed;
  left: 0;
  top: 0;
  transform: translateX(-10000%);
  width: 100%;
  height: 100%;
  opacity: 0;
  /* overflow: scroll; */
}

/* 展示画布 */

.canvas-box {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 600rpx;
  height: 900rpx;
  /* background-color: cyan; */
}

.form-img {
  position: absolute;
  right: 0;
  top: 0;
  width: 80rpx;
  height: 80rpx;
  transform: translate(50%, -50%);
}

.canvas-box image {
  width: 600rpx;
  height: 900rpx;
}

.form-img image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  overflow: hidden;
}

/* 待签名前的定价 */

.wait-confirm {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
  padding: 0rpx 30rpx;
  background-color: #fff;
  height: 80rpx;
}
.wait-money{
  color: #ff5062;
  font-size: 36rpx;
}
.wait-money::before{
  content: '￥';
  font-size: 28rpx;
}